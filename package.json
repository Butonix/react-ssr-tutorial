{
  "private": true,
  "scripts": {
    "build:production": "NODE_ENV=production NODE_PATH=. webpack --mode production",
    "build:dev:client": "NODE_ENV=local NODE_PATH=. webpack --mode=development --hide-modules --config webpack/client.config.js",
    "build:dev:server": "NODE_ENV=local NODE_PATH=. webpack --mode=development --hide-modules --config webpack/server.config.js --watch",
    "start": "npm run build:dev:client && npm-run-all --parallel --print-name build:dev:server start:dev",
    "start:dev": "nodemon index --watch dist/server.js",
    "lint": "eslint --ext=.js,.jsx ./src"
  },
